# bashrc additions for xmonad
alias retina27='~/src/xmonad/install Xdefaults-retina27 \
              && (urxvt -e screen -x main &) \
              && kill $PPID'
alias retina15='~/src/xmonad/install Xdefaults-retina15.4 \
              && (urxvt -e screen -x main &) \
              && kill $PPID'
